
<%= simple_form_for([:admin, @event]) do |f| %>
  <%= render "/shared/error_messages", target: @event  %>
    <fieldset>
    <legend></legend>
    <%= f.input :name %>
    <%= f.input :date %>
    <%= f.input :address %>
    <%= f.input :contact %>
    <%= f.input :description %>
    <div class="form-group">
      <%= f.submit t("common.save"), class: "btn btn-primary" %> 
      <%= link_to t("common.cancel"), admin_events_path %>
  </div>
  </fieldset>
<% end %>